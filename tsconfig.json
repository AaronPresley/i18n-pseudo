/* Visit https://aka.ms/tsconfig.json to read more about this file */
{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "noImplicitAny": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "skipLibCheck": true,
    "strict": false,
    "target": "es5"
  },
  "include": [
    "src/**/*"
  ]
}
